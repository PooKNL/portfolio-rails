<h1 id="page_title"> My Projects </h1>

<div id="articles_wrapper" class="skinny_wrapper">
  <div class="project_article">
    <% @projects.each do |project| %>
      <h3><%= link_to project.title, project %> </h3>
      <p class="date"> <%= project.created_at.strftime("%A,%b, %d") %> </p>
      <% end %>
  </div>


  <div class="pagination">
    <span class="previous_page disabled">
    Previous</span>
    <em class="current">1</em>
    <a rel="next" href="#">2</a>
    <a class="next_page" href="#">Next</a>
  </div>

</div>
